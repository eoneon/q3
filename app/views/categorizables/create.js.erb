$("#<%= dynamic_id(name: 'collection', klass: klass_name(@categorizable), child_klass: nil) %>").append('<%= j render "categorizables/categorizable", category: @category, sub_category: @sub_category %>');
$("#<%= dynamic_id(name: 'form-new-render', klass: @category, child_klass: klass_name(@categorizable)) %>").html("<%= j render 'categorizables/forms/form', category: @category, categorizable: konstant(@categorizable).new, form_id: dynamic_id(name: 'form-new', klass: @category, child_klass: klass_name(@categorizable)) %>");
