<div class="card bg-light mb-3 p-2" id='<%= "#{to_snake(origins.first)}-new" %>'>
  <%= render "product_kinds/forms/new", origin: to_konstant('product_kind').new, form_id: "#{to_snake(origins.first)}-new-form-id" %>
</div>
<%= render "product_kinds/import", url: import_product_parts_path %>

<div class="card align-items-center bg-light m-0 p-0">
  <div class="card-body bg-light m-0 p-1">
    <span><%= obj_to_cap_hyph_str(origins.first).pluralize %></span>
  </div>
</div>

<div class="list-group" id='<%= "#{to_snake(origins.first)}-tab-index" %>' role="tablist">
  <%= render "product_kinds/tabs", origins: origins %>
</div>
