<div class="card card-body p-3 bg-light">
  <div>
    <span class="card-title font-weight-light">New Element Type</span>
    <button type="button" class="close float-right close-form">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <div id="<%= dynamic_id(name: 'form-new-render', klass: elementable, child_klass: 'element_kind') %>">
    <%= render "element_kinds/forms/form", elementable: elementable, element_kind: ElementKind.new, form_id: dynamic_id(name: 'form-new', klass: elementable, child_klass: 'element_kind')  %>
  </div>
</div>
