<div class="card card-body p-3 bg-light">
  <div>
    <span class="card-title font-weight-light">Associate Existing Element Type</span>
    <button type="button" class="close float-right close-form">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <div id="<%= dynamic_id(name: 'form-new-render', klass: elemental, child_klass: 'element_group') %>">
    <%= render "element_groups/form", elemental: elemental, element_group: ElementGroup.new, form_id: dynamic_id(name: 'form-new', klass: elemental, child_klass: 'element_group')  %>
  </div>
</div>
