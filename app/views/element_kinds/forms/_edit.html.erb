<div class="card card-body p-3 bg-light">
  <div>
    <span class="card-title font-weight-light">Edit Element Type</span>
    <button type="button" class="close float-right close-form">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <div id="<%= dynamic_id(name: 'form-edit-render', klass: elementable, child_klass: element_kind) %>">
    <%= render "element_kinds/forms/form", elementable: elementable, element_kind: element_kind, form_id: dynamic_id(name: 'form-edit', klass: elementable, child_klass: element_kind)  %>
  </div>
</div>
