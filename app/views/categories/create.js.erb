$("#<%= new_dom_ref(@form_id, 'form', nil) %>").html("<%= j render 'categories/forms/form', category: Category.new, form_id: @form_id %>");
enableNewInputs("#category-new");
$("#category-tab-index").html("<%= j render 'categories/tabs', categories: @categories %>");
$(".list-group-item").removeClass("active");
$("#<%= dom_ref(@category, 'tab-item') %>").addClass("active");

$("#category-show").html("<%= j render 'categories/show', category: @category %>");
$("#category-element_kind-new").html("<%= j render 'element_kinds/forms/form', elementable: @category, element_kind: ElementKind.new, form_id: 'category-element_kind-new-form-id' %>");
$("#category-element_group-add").html("<%= j render 'element_groups/forms/form', elementable: @category, element_group: ElementGroup.new, form_id: 'category-element_group-add-form-id' %>");
configShow("#category-edit");
getDeleteBtn("#category-edit").attr("href", "<%= category_path(@category) %>");
