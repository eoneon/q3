$("#<%= dom_ref(@form_id) %>").replaceWith("<%= j render 'product_parts/forms/form', product_part: @sub_part, url: product_part_sub_part_path(@product_part, @sub_part), target: @product_part.item_groups.first, form_id: @form_id %>");
var tab_item = $("#<%= dom_ref(@sub_part, 'tab-item') %>");
$(tab_item).find(".item-type").text("<%= @sub_part.type %>");
$(tab_item).find(".item-name").text("<%= @sub_part.name %>");
