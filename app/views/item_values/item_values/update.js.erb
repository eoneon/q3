$("#<%= new_dom_ref(@form_id, 'form', nil) %>").html("<%= j render 'item_values/forms/form', item_value: @item_value, url: item_value_path(@item_value), form_id: @form_id %>");
$("<%= dom_ref('#item_value', @item_value.id, 'tab-item') %>").text("<%= @item_value.name %>");
enableToggleInput("#<%= @form_id %>");
getDeleteBtn("#<%= @form_id %>").toggleClass("disabled");
