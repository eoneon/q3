</div>
<%= render "product_kinds/import", url: import_item_values_path %>

<div class="card align-items-center bg-light m-0 p-0">
  <div class="card-body bg-light m-0 p-1">
    <span>Item-Values</span>
  </div>
</div>

<div class="list-group" id='<%= "item_value-tab-index" %>' role="tablist">
  <%= render "item_values/tabs", item_values: item_values %>
</div>
