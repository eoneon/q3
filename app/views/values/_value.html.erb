<div id="<%= dyno_id(n1: 'value', k1: value, tag: 'index-item') %>">
  <div class="card">
    <div class="card-header py-0 px-3">
      <%= render "values/header/header", field_group: field_group, value: value %>
    </div>

    <div class="collapse" id="<%= dyno_id(n1: 'field_group', k1: field_group, n2: 'value-properties', k2: value, tag: 'edit') %>">
      <%= render "values/forms/properties", field_group: field_group, value: value %>
    </div>
  </div>
</div>
