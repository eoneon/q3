$("#<%= dynamic_id(name: 'collection-render', klass: @field.field_groups.first, child_klass: 'values') %>").html('<%= j render "values/value", field_group: @field.field_groups.first, field: @field %>');
$("#<%= dynamic_id(name: 'form-new-render', klass: @field.field_groups.first, child_klass: 'value') %>").html("<%= j render 'values/forms/form', field: @field, value: Value.new, form_id: dynamic_id(name: 'form-new', klass: 'value', child_klass: nil) %>");
