$("#<%= dynamic_id(name: 'collection', klass: @field_group, child_klass: 'values') %>").append("<%= j render 'values/value/value', field_group: @field_group, field: @field, value: @value %>");
$("#<%= dynamic_id(name: 'form-new-render', klass: @field_group, child_klass: 'value') %>").html("<%= j render 'values/forms/form', field: @field, value: Value.new, form_id: dynamic_id(name: 'form-new', klass: 'value', child_klass: nil) %>");
