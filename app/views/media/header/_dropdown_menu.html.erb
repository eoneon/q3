<div class="dropdown-menu">
  <a class="dropdown-item disabled" data-toggle="collapse" href='#<%= dom_ref(target, "edit") %>'>Edit <%= obj_to_cap_hyph_str(target) %></a>
  <% self_join_assocs(target, origin).each do |sub_target| %>
    <a class="dropdown-item" data-toggle="collapse" href='#<%= dom_ref(target, "#{sub_target}-item_group-add") %>'>Add <%= obj_to_cap_hyph_str(sub_target) %></a>
  <% end %>
</div>
