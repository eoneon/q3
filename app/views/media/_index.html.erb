<% if origin.present? && origin.item_groups.any? && origin.grouped_subklass(str_to_camel(target)).any? %>
  <% origin.grouped_subklass(str_to_camel(target)).each do |item_group| %>
    <%= render "media/show", origin: origin, target: item_group.target, item_group: item_group  %>
  <% end %>
<% end %>
