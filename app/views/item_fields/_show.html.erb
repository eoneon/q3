<div class="card border m-1 border-dark" id="<%= dom_ref(item_field, 'show') %>">
  <div class="card-header">
    <%= render "item_fields/header", item_field: item_field, obj_ref: dom_ref(item_field) %>
  </div>

  <div class="card-body p-1 collapse" id="<%= dom_ref(item_field, 'body') %>">
    <div id='<%#= dom_ref(item_field, "#{to_snake(item_field.sti_value_type)}-index") %>'>
      <%#= render "item_values/item_fields/index", origin: item_field %>
    </div>
  </div>
</div>
