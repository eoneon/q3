<div class="card card-body p-3 bg-light">
  <div>
    <span class="card-title font-weight-light">Edit <%= element.name %></span>
    <button type="button" class="close float-right close-form">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <div id="<%= dynamic_id(name: 'form-edit-render', klass: element_kind, child_klass: element) %>">
    <%= render "elements/forms/form", element_kind: element_kind, element: element, form_id: dynamic_id(name: 'form-edit', klass: element_kind, child_klass: element)  %>
  </div>
</div>
