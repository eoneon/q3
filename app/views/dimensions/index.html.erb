<h1>Dimension#index</h1>

<% if @dimensions.empty? %>
  <p><strong>No Dimension</strong></p>
<% end %>

<p>
  Download:
  <%= link_to "Export dimensions to CSV", dimensions_path(format: "csv") %> |
  <%= link_to "Export dimensions to Excel", dimensions_path(format: "xls") %>
</p>

<h4>Import Fields</h4>
<%= form_tag import_dimensions_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>

<%= link_to "New Dimension", new_dimension_path %>

<table class="table table-striped table-hover">
  <thead>
    <th>ID</th>
    <th>Name</th>
  </thead>
  <tbody>
    <%= render @dimensions %>
  </tbody>
</table>
