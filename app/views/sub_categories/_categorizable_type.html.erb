<% category.categorizable_types.each do |categorizable_type| %>
  <!-- CONTAINER FOR EACH SPECIFIC SUB-CATEGORY and it's fields/values -->
  <div class="card bg-dark">
    <div class="card-header py-0">
      <%= render "sub_categories/categorizable_type/header", category: category, categorizable_type: categorizable_type %>
    </div>

    <div class="collapse close-toggle-form" id="<%= dynamic_id(name: 'form-new-target', klass: category, child_klass: klass_name(categorizable_type)) %>" data-parent="#accordion-nav-menu">
      <%= render "sub_categories/categorizable_type/new", category: category, categorizable_type: categorizable_type %>
    </div>

    <div class="collapse close-toggle-form" id="<%= dynamic_id(name: 'form-add-target', klass: category, child_klass: klass_name(categorizable_type)) %>" data-parent="#accordion-nav-menu">
      <%= render "sub_categories/categorizable_type/add", category: category, categorizable_type: categorizable_type %>
    </div>

    <div class="collapse <%= dynamic_id(name: 'collection-target', klass: category, child_klass: kollection_name(categorizable_type)) %>" id="<%= dynamic_id(name: 'accordion', klass: klass_name(categorizable_type), child_klass: nil) %>" data-parent="#<%= dynamic_id(name: 'accordion', klass: category, child_klass: 'sub_category') %>">

      <div class="card-body p-3">
        <div id="<%= dynamic_id(name: 'collection', klass: kollection_name(categorizable_type), child_klass: nil) %>">

          <% category.sorted_subcategories(categorizable_type).each do |sub_category| %>
            <!-- kill -->
            <div id="<%= dynamic_id(name: 'accordion', klass: sub_category, child_klass: nil) %>">
              <!-- Q: why isn't my new dimension/categorizable prepending? -->
              <div id="<%= dynamic_id(name: 'collection-render', klass: sub_category, child_klass: kollection_name(categorizable_type)) %>">
                <!-- CONTAINER FOR EACH SPECIFIC CATEGORIZABLE, i.e., FRAME AND IMAGE -->
                <%= render "sub_categories/categorizable/categorizable", category: category, sub_category: sub_category %>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
<% end %>
