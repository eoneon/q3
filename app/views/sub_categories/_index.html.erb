<% if origin_sti_item_groups = origin_sti_item_groups?(origin, type) %>
  <h6 class="card-title py-0 my-0"><%= card_title(origin, type) %></h6>
  <% origin_sti_item_groups.each do |item_group| %>
    <%= render "sub_categories/show", origin: origin, target: item_group.target, item_group: item_group, obj_ref: dom_ref(origin, item_group.target, item_group) %>
  <% end %>
<% end %>
