<%= form_with model: element_group, url: [elementable, element_group], id: form_id do |f| %>
  <div class="form">
    <%= hidden_field_tag :form_id, form_id %>
    <div class="form-row align-items-center">
      <%= render "element_kinds/forms/input_group", element_kind: element_kind, f: f, form_id: form_id %>
    </div>
  </div>
<% end %>
